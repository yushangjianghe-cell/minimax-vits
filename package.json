{
  "name": "koishi-plugin-minimax-vits",
  "description": "自用，使用 minimax 国际版生成语音，适配 chatluna",
  "version": "1.5.3",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./console": {
      "types": "./lib/console.d.ts",
      "import": "./lib/console.jsx"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "readme.md"
  ],
  "license": "MIT",
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "minimax",
    "tts",
    "vits",
    "chatluna",
    "console"
  ],
  "peerDependencies": {
    "@koishijs/plugin-console": "^5.16.0",
    "koishi": "^4.18.10"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@koishijs/client": "^5.16.0",
    "@types/node": "^20.0.0",
    "@types/form-data": "^2.5.0",
    "tsc-alias": "^1.8.0",
    "typescript": "^5.0.0",
    "vue": "^3.3.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json && node fix-imports.js",
    "build:dev": "tsc --watch",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run build"
  },
  "inject": {
    "optional": [
      "console",
      "chatluna"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yushangjianghe-cell/minimax-vits.git"
  },
  "bugs": {
    "url": "https://github.com/yushangjianghe-cell/minimax-vits.git"
  },
  "homepage": "https://github.com/yushangjianghe-cell/minimax-vits.git",
  "koishi": {
    "description": {
      "en": "MiniMax VITS TTS plugin with console UI",
      "zh": "自用插件。带控制台界面的MiniMax VITS TTS插件"
    },
    "schema": true,
    "service": {
      "implements": [
        "minimax-vits"
      ]
    }
  }
}