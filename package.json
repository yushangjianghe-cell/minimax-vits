{
  "name": "koishi-plugin-minimax-vits",
  "description": "使用 minimax 国际版生成语音，适配 chatluna",
  "version": "1.4.0",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./console": {
      "import": "./lib/console.jsx"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "dist",
    "client",
    "readme.md"
  ],
  "license": "MIT",
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "minimax",
    "tts",
    "vits",
    "chatluna",
    "console"
  ],
  "peerDependencies": {
    "koishi": "^4.18.10",
    "@koishijs/plugin-console": "^5.16.0"
  },
  "dependencies": {
    "@langchain/core": "^0.1.0",
    "koishi-plugin-chatluna": "^1.0.0"
  },
  "inject": {
    "optional": [
      "console",
      "chatluna"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "@koishijs/client": "^5.16.0",
    "vue": "^3.3.0",
    "@vue/tsconfig": "^0.5.0"
  },
  "scripts": {
    "build:server": "tsc && tsc-alias",
    "build:client": "koishi-console build",
    "build": "yarn build:server && yarn build:client",
    "dev": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yushangjianghe-cell/minimax-vits.git"
  },
  "bugs": {
    "url": "https://github.com/yushangjianghe-cell/minimax-vits.git"
  },
  "homepage": "https://github.com/yushangjianghe-cell/minimax-vits.git",
  "koishi": {
    "description": {
      "en": "MiniMax VITS TTS plugin with console UI",
      "zh": "带控制台界面的MiniMax VITS TTS插件"
    },
    "service": {
      "implements": ["minimax-vits"]
    },
    "browser": true
  }
}